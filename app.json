{
  "expo": {
    "userInterfaceStyle": "light",
    "name": "BikeInspec",
    "slug": "BikeInspec",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./res/images/icon.png",
    "splash": {
      "image": "./res/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "infoPlist": {
        "UIUserInterfaceStyle": "Light"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./res/images/logoBI_sm.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "light"
    },
    "web": {
      "favicon": "./res/images/favicon.png"
    }
  }
}
